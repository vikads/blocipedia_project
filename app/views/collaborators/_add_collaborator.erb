<%=  form_for [wiki, Collaborator.new] do |f| %>

  <div class="form-group">
    <%= email_field_tag :collaborator, " ", class: 'form-control', placeholder: 'Enter collaborator email here.' %>
  </div>

  <div class="form-group">
    <%= f.submit "Save collaborator", class: 'btn btn-succes' %>
  </div>

<% end %>
